{
  "name": "core-library",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libraries/core-library/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libraries/core-library",
        "tsConfig": "libraries/core-library/tsconfig.lib.json",
        "packageJson": "libraries/core-library/package.json",
        "main": "libraries/core-library/src/index.ts",
        "assets": [
          "libraries/core-library/*.md"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "libraries/core-library/jest.config.ts"
      }
    }
  }
}
